  /*Checkboxes styles*/
  input[type="checkbox"] { display: none; }

  input[type="checkbox"] + label {
    width: 90%;
    display: inline-block;
    position: relative;
    padding-left: 36px;
    margin-bottom: 20px;
    color: #ddd;
    cursor: pointer;

    height: 40px;
