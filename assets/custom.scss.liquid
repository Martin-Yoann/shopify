.slide2-logos { animation-delay: 4s }
.slide3-logos { animation-delay: 8s }
.slide4-logos { animation-delay: 12s }
.slide5-logos { animation-delay: 16s }
.slide6-logos { animation-delay: 20s }

@keyframes fade
{
  0%   {opacity: 0}
  5%   {opacity: 1; top: -10%; }
  16% { opacity: 1; top: -10%; }
  21% { opacity: 0}
  50% { opacity: 0}
  75% { opacity: 0}
  100% { opacity: 0}
}

@keyframes fadelogos
{
  0%   {opacity: 0.25}
  5%   {opacity: 1}
  16% { opacity: 1}
  21% { opacity: 0.25}
  50% { opacity: 0.25}
  75% { opacity: 0.25}
  100% { opacity: 0.25}
}











