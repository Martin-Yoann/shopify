.genki-modal-open { display: inline-block; border-bottom: 1px solid black; }
.genki-modal-open:hover { cursor: pointer; opacity: .75; border-bottom: 1px solid #666;  } 


.genki-modal-overlay {
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0; right: 0; bottom: 0; top: 0;
  background: rgba(#42264a,.75);
  z-index: 9999;

  @include ease(.6s);
  opacity: 0;
  visibility: hidden;
  &.active {
    opacity: 1;
    visibility: visible;
  }
}
.genki-modal {
  position: fixed;
  
  background: #fbfbfb;
  margin: 0 auto;
  left: 50%;
  width: 260px;
  margin-left: -130px;
  z-index: 99999;
  padding: 30px;
  height: auto;
  top: 15%;
  @include ease(.6s);
  opacity: 0;
  visibility: hidden;
  &.active {
    top: 10%;
    opacity: 1;
    visibility: visible;
  }
  @include breakpoint(xs) {
    top: 10%;
    padding: 15px;
    margin: 0;
    left: 5%;
  }
  img {
    display: block;
    max-width: 200px;
  }
}


.modalclose {
      position: absolute;
    right: 0px;
    top: -46px;
    width: 32px;
    height: 32px;
    opacity: 1;
    &:hover { cursor: pointer; }
}
.modalclose:hover {
  opacity: 1;
}
.modalclose:before, .modalclose:after {
  position: absolute;
  left: 15px;
  content: ' ';
  height: 33px;
  width: 2px;
  background-color: white;
}
.modalclose:before {
  transform: rotate(45deg);
}
.modalclose:after {
  transform: rotate(-45deg);
}






